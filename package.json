{
  "name": "eversh2op",
  "version": "1.2.2",
  "type": "module",
  "description": "A shopping cart platform with Express, React and Postgres",
  "workspaces": [
    "packages/*",
    "extensions/*"
  ],
  "scripts": {
    "dev": "turbo watch dev --filter=@amohajewellery/amohajewellery --filter=agegate",
    "start": "turbo watch start --filter=@amohajewellery/amohajewellery --filter=agegate",
    "build": "turbo watch build --filter=@amohajewellery/amohajewellery --filter=agegate",
    "setup": "amohajewellery install",
    "build-no-compile": "node packages/amohajewellery/dist/bin/build/index.js",
    "start:debug": "node ./packages/amohajewellery/dist/bin/start/index.js --debug",
    "test": "ALLOW_CONFIG_MUTATIONS=true NODE_OPTIONS=--experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install"
  },
  "author": "The Nguyen (https://amohajewellery.io)",
  "license": "GNU GENERAL PUBLIC LICENSE 3.0",
  "devDependencies": {
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.29",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "copyfiles": "^2.4.1",
    "cypress": "^13.15.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "2.8.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swc-minify-webpack-plugin": "^2.1.3",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.3.2",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "dependencies": {
    "@amohajewellery/amohajewellery": "file://packages/amohajewellery"
  },
  "packageManager": "npm@9.6.7"
}
