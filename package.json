{
  "name": "amohajewellery",
  "version": "1.0.0-beta",
  "type": "module",
  "description": "A shopping cart platform with Express, React and Postgres",
  "files": [
    "bin",
    "src",
    "dist",
    ".babelrc",
    "babel.config.js",
    "tsconfig.json"
  ],
  "bin": {
    "amohajewellery": "./bin/amohajewellery"
  },
  "workspaces": [
    "packages/*",
    "extensions/*"
  ],
  "main": "./dist/packages/amohajewellery/bin/start/index",
  "scripts": {
    "dev-ts": "rimraf ./dist && tsc -p tsconfig.json",
    "dev": "amohajewellery dev",
    "setup": "amohajewellery install",
    "build": "amohajewellery build",
    "start": "amohajewellery start",
    "user:create": "amohajewellery user:create",
    "user:changePassword": "amohajewellery user:changePassword",
    "start:debug": "amohajewellery start --debug",
    "test": "jest --workspace=@amohajewellery/amohajewellery",
    "cypress": "cypress run",
    "lint": "eslint --quiet --ext .jsx ",
    "prepare": "husky install",
    "typescript-build": "rimraf dist && tsc && copyfiles ./**/*.graphql dist/ && copyfiles ./pages dist/"
  },
  "author": "The Nguyen (https://amohajewellery.io)",
  "license": "GNU GENERAL PUBLIC LICENSE 3.0",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "babel-jest": "^28.1.1",
    "copyfiles": "^2.4.1",
    "cypress": "^13.15.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "2.8.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tailwindcss": "^3.2.1",
    "ts-patch": "^3.3.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.5.0",
    "typescript-transform-paths": "^3.5.5"
  }
}
